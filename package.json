  "devDependencies": {
    "@types/react": "^16.7.4",
    "@types/react-dom": "^16.0.9",
    "@types/react-router-dom": "^4.3.1",
    "concurrently": "^4.0.1",
    "cross-env": "^5.2.0",
    "electron": "^4.0.0-beta.7",
    "electron-rebuild": "^1.8.2",
    "file-loader": "^2.0.0",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.9.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^2.1.5",
    "sass-loader": "^7.0.3",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.21.0",
    "ts-loader": "^5.3.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "autoprefixer": "^9.3.1",
    "css-loader": "^1.0.1",
    "electron-devtools-installer": "^2.2.4",
    "pluralize": "^7.0.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-semantic-toasts": "^0.3.9",
    "reflect-metadata": "^0.1.12",
    "semantic-ui-calendar-react": "^0.11.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.83.0",
    "sqlite3": "^4.0.4",
    "typeorm": "^0.2.8"
  },
  "scripts": {
    "predev": "npm run compile:main",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run compile -- --watch\" \"electron .\"",
    "compile": "npm run compile:main && npm run compile:renderer",
    "compile:main": "webpack --config ./task/webpack.main.js --hide-modules",
    "compile:renderer": "webpack --config ./task/webpack.renderer.js",
    "compile:prod": "cross-env NODE_ENV=production npm run compile",
    "postinstall": "yarn run electron-rebuild"
  }
}
